{
  "name": "结构化页面提取器",
  "description": "对页面进行结构化提取",
  "version": "1.0",
  "manifest_version": 2,
  "options_page": "html/config.html",
  "permissions":
    [
      "tabs",
      "activeTab",
      "notifications"
    ],
    "background": 
  {
    "scripts":["js/background.js"]
  },
  "icons": {
    "16": "icon/icon.png"
},
"browser_action": 
  {
    "default_icon" : "icon/icon.png",
    "default_title": "结构化提取",
    "default_popup" : "html/popup.html"
  },
"content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["js/jquery-1.11.3.min.js"] 
  }],
"omnibox": { "keyword" : "search" }
}



